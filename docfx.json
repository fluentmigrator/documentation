{
  "metadata": [
    {
      "src": [
        {
          "src": "fm-v2.x/src",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "FluentMigrator.DotNet.Cli/**",
            "FluentMigrator.Console/**",
            "FluentMigrator.MSBuild/**"
          ]
        }
      ],
      "dest": "api/v2.x",
      "filter": "api-filter.yml",
      "disableGitFeatures": true,
      "properties": {
        "TargetFramework": "net461"
      }
    },
    {
      "src": [
        {
          "src": "fm-v3.x/src",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "FluentMigrator.DotNet.Cli/**",
            "FluentMigrator.Console/**",
            "FluentMigrator.MSBuild/**"
          ]
        }
      ],
      "dest": "api/v3.x",
      "filter": "api-filter.yml",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "net461"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**.yml", "**.md" ],
        "src": "api/v2.x",
        "dest": "api/v2.x",
        "group": "v2.x"
      },
      {
        "files": [ "**.yml", "**.md" ],
        "src": "api/v3.x",
        "dest": "api/v3.x"
      },
      {
        "files": [
          "api/*.md",
          "api/toc.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/v2.x/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
        "group": "v2.x"
      },
      {
        "files": [
          "apidoc/v3.x/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "FluentMigrator documentation",
      "_appFooter": "<table border='0'><tr><td><span>Copyright Â© 2018 Fluent Migrator Project<br>Generated by <strong>DocFX</strong></span></td><td><a rel='license' href='http://creativecommons.org/licenses/by-sa/4.0/'><img alt='Creative Commons License' style='border-width:0' src='https://i.creativecommons.org/l/by-sa/4.0/88x31.png' /></a><br /><span xmlns:dct='http://purl.org/dc/terms/' property='dct:title'>FluentMigrator Documentation</span> by <a xmlns:cc='http://creativecommons.org/ns#' href='https://fluentmigrator.github.io' property='cc:attributionName' rel='cc:attributionURL'>FluentMigrator Project</a> is licensed under a <a rel='license' href='http://creativecommons.org/licenses/by-sa/4.0/'>Creative Commons Attribution-ShareAlike 4.0 International License</a>.</td></tr></table>",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/fluentmigrator/documentation",
        "branch": "master",
        "apiSpecFolder": "apidoc"
      }
    },
    "fileMetadata": {
      "_gitContribute": {
        "api/v2.x/**": {
          "repo": "https://github.com/fluentmigrator/documentation",
          "branch": "master",
          "apiSpecFolder": "apidoc/v2.x"
        },
        "api/v3.x/**": {
          "repo": "https://github.com/fluentmigrator/documentation",
          "branch": "master",
          "apiSpecFolder": "apidoc/v3.x"
        }
      }
    }
  }
}
